{
  "name": "react-native-detect-maestro",
  "version": "1.0.5",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "react-native": "src",
  "source": "src",
  "peerDependencies": {
    "react-native": ">=0.65.0"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    }
  },
  "scripts": {
    "prebuild": "rm -fr dist",
    "ts": "tsc --noEmit",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "prepack": "yarn build"
  },
  "author": "David <4661784+retyui@users.noreply.github.com>",
  "description": "Detect Maestro in your React Native App",
  "homepage": "https://github.com/retyui/react-native-detect-maestro#readme",
  "bugs": "https://github.com/retyui/react-native-detect-maestro/issues",
  "repository": "https://github.com/retyui/react-native-detect-maestro",
  "files": [
    "dist",
    "src",
    "android",
    "ios",
    "DetectMaestro.podspec",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "codegenConfig": {
    "name": "DetectMaestro",
    "type": "all",
    "jsSrcsDir": "src",
    "android": {
      "javaPackageName": "com.retyui.detectmaestro"
    }
  },
  "devDependencies": {
    "react-native": "^0.72.3",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "react",
    "react-native",
    "react-native-component",
    "android",
    "ios",
    "maestro",
    "e2e",
    "is-e2e",
    "is-maestro"
  ]
}
